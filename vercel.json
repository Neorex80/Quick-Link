{
  "version": 2,
  "name": "quicklink",
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.go"
    }
  ],
  "env": {
    "GO_VERSION": "1.21"
  },
  "functions": {
    "main.go": {
      "runtime": "@vercel/go"
    }
  }
}
